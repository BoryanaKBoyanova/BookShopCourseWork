@using BookShopCourseWork.Models
@{
    ViewData["ActiveTab"] = "Publishers";
    ViewData["ActivePage"] = "ViewAllPublishers";
}
@model List<Publisher>
<div class="mt-3 ml-3 mb-3 mr-3">
    <h4>View all publishers</h4>
    <hr>
     <table class="table table-striped table-primary">
            <thead>
                <tr>
                    <th scope="col">Id</th>
                    <th scope="col">Publisher name</th>
                </tr>
            </thead>
            <tbody>
                @foreach (Publisher p in Model)
                {
                    <tr><td>@p.Id</td><td>@p.PublisherName</td></tr>
                }
            </tbody>
        </table>
</div>