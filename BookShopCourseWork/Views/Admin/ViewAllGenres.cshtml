@using BookShopCourseWork.Models
@{
    ViewData["ActiveTab"] = "Genres";
    ViewData["ActivePage"] = "ViewAllGenres";
}
@model List<Genre>
<div class="mt-3 ml-3 mb-3 mr-3">
    <h4>View all genres</h4>
    <hr>
     <table class="table table-striped table-primary">
            <thead>
                <tr>
                    <th scope="col">Id</th>
                    <th scope="col">Genre name</th>
                </tr>
            </thead>
            <tbody>
                @foreach (Genre g in Model)
                {
                    <tr><td>@g.Id</td><td>@g.GenreName</td></tr>
                }
            </tbody>
        </table>
</div>